{
  "name": "marx/phar-self-updater",
  "description": "Library for phar self updates by github",
  "keywords": ["phar", "update"],
  "type": "library",
  "authors": [
    {
      "name": "Marc Siebeneicher",
      "email": "marc.siebeneicher@trivago.com",
      "role": "developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",

    "symfony/finder": "~2.7",
    "symfony/filesystem": "~2.7",

    "psr/http-message": "~1.0",
    "guzzlehttp/guzzle": "~5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload":
  {
    "psr-4": {
      "PSU\\": ["src/"],
      "PSUTest\\": ["test/"]
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "stable"
}